

public class Student {
String firstName;
String lastName;
String studentNumber;
int grade;
String[] timetable;
//Create a new student with the given parameters.

public Student (String first, String last , String studentNumber, int grade );
super();
this.first = first;
this.last = last;
this.studentNumber =studentNumber;
this.grade = grade;

}
// Replace  the existing first name with newFirstName //
 public class static void setFirstName(string newFirstName){
this.firstName = newFirstName;
}

public class  String getfirstName(){
return this.firstname;
}

public class  void setLastName(string newlastName){
this.lastName= newLastName;
}

public class  String getlastName(){
return this.lastname;
}

public class void setGrade(int newGrade){
this.grade = newGrade;
}

public class  String getgrade(){
return this.grade;
}


public class String getFullName(){
String result = firstName + " "  +lastName ;
result result;
}


public class static void mail (String [], args){
Student s = new Student (" Pragati " , "paliwal" , "12345" , 3);
System.out.println("Student");
}
}



